@article{wattsCoupledClusterSinglesDoubles1994,
  title = {Coupled-{{Cluster}} Singles, Doubles, and Triples Calculations with Hartree-Fock and Brueckner Orbital Reference Determinants: {{A}} Comparative Study},
  shorttitle = {Coupled-{{Cluster}} Singles, Doubles, and Triples Calculations with Hartree-Fock and Brueckner Orbital Reference Determinants},
