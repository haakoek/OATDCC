@article{helgakerSecondquantizationApproachAnalytical1984,
  title = {A Second-Quantization Approach to the Analytical Evaluation of Response Properties for Perturbation-Dependent Basis Sets: {{BASIS SETS}}},
  shorttitle = {A Second-Quantization Approach to the Analytical Evaluation of Response Properties for Perturbation-Dependent Basis Sets},
  author = {Helgaker, Trygve U. and Alml{\"o}f, Jan},
  year = {1984},
