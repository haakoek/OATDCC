@article{vidalDysonOrbitalsFcCVSEOMCCSD2020,
  title = {Dyson Orbitals within the Fc-{{CVS-EOM-CCSD}} Framework: Theory and Application to {{X-ray}} Photoelectron Spectroscopy of Ground and Excited States},
  shorttitle = {Dyson Orbitals within the Fc-{{CVS-EOM-CCSD}} Framework},
  author = {Vidal, Marta L. and Krylov, Anna I. and Coriani, Sonia},
  year = {2020},
