@article{sokolovQuantumOrbitaloptimizedUnitary2020,
  title = {Quantum Orbital-Optimized Unitary Coupled Cluster Methods in the Strongly Correlated Regime: {{Can}} Quantum Algorithms Outperform Their Classical Equivalents?},
  shorttitle = {Quantum Orbital-Optimized Unitary Coupled Cluster Methods in the Strongly Correlated Regime},
  author = {Sokolov, Igor O. and Barkoutsos, Panagiotis Kl. and Ollitrault, Pauline J. and Greenberg, Donny and Rice, Julia and Pistoia, Marco and Tavernelli, Ivano},
  year = {2020},
