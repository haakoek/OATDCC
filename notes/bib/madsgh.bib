@article{lawsVelocityMapImaging2022,
  title = {Velocity Map Imaging Spectroscopy of {{C}} {\textsubscript{2}} {{H}} {\textsuperscript{-}} and {{C}} {\textsubscript{2}} {{D}} {\textsuperscript{-}} : {{A}} Benchmark Study of Vibronic Coupling Interactions},
