  primaryclass = {physics},
  pages = {043303},
  issn = {2470-0045, 2470-0053},
  doi = {10.1103/PhysRevE.94.043303},
  urldate = {2023-09-11},
  abstract = {Explicit symplectic integrators have been important tools for accurate and efficient approximations of mechanical systems with separable Hamiltonians. For the first time, the article proposes for arbitrary Hamiltonians similar integrators, which are explicit, of any even order, symplectic in an extended phase space, and with pleasant long time properties. They are based on a mechanical restraint that binds two copies of phase space together. Using backward error analysis, KAM theory, and additional multiscale analysis, an error bound of \$\textbackslash mathcal\{O\}(T\textbackslash delta\^l \textbackslash omega)\$ is established for integrable systems, where \$T\$, \$\textbackslash delta\$, \$l\$ and \$\textbackslash omega\$ are respectively the (long) simulation time, step size, integrator order, and some binding constant. For non-integrable systems with positive Lyapunov exponents, such an error bound is generally impossible, but satisfactory statistical behaviors were observed in a numerical experiment with a nonlinear Schr\textbackslash "\{o\}dinger equation.},
  archiveprefix = {arxiv},
  keywords = {Mathematics - Dynamical Systems,Mathematics - Numerical Analysis,Physics - Classical Physics,Physics - Computational Physics}
}

