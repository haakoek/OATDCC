@article{liuUnitaryCoupledclusterBased2021,
  title = {Unitary Coupled-Cluster Based Self-Consistent Polarization Propagator Theory: {{A}} Quadratic Unitary Coupled-Cluster Singles and Doubles Scheme},
  shorttitle = {Unitary Coupled-Cluster Based Self-Consistent Polarization Propagator Theory},
  author = {Liu, Junzi and Cheng, Lan},
