@article{huaTransientXrayAbsorption2019,
  title = {Transient {{X-ray Absorption Spectral Fingerprints}} of the {{S1 Dark State}} in {{Uracil}}},
  author = {Hua, Weijie and Mukamel, Shaul and Luo, Yi},
  year = {2019},
  month = nov,
  journal = {J. Phys. Chem. Lett.},
  volume = {10},
  number = {22},
  pages = {7172--7178},
  publisher = {{American Chemical Society}},
  doi = {10.1021/acs.jpclett.9b02692},
  urldate = {2020-09-15},
  abstract = {Low-lying dark n{$\pi$}* states play an important role in many photophysical and photochemical processes of organic chromophores. Transient X-ray absorption spectroscopy (TXAS) provides a powerful technique for probing the dynamics of valence states by exciting the electrons into high-lying core excited states. We employ multiconfigurational self-consistent field calculations to investigate the TXAS of uracil along its nonradiative photodecay pathways. An open issue is whether dark n{$\pi$}* state S1 (n is the lone pair localized on an oxygen atom) is accessible when bright {$\pi\pi$}* state S2 is selectively excited. Vertical core excitations were calculated along the potential energy surfaces of the three lowest states, S0\textendash S2, interpolated between two minima and two minimum-energy conical intersections. Computed TXAS data from the C, N, and O K edges show distinct spectral fingerprints of the dark state in all spectral regimes. At the O 1s edge, the n{$\pi$}* state has a very strong absorption at 526\textendash 527 eV, while at the C (N) 1s edge, by contrast, there is almost zero (very weak) absorption at 279\textendash 282 eV (397\textendash 398 eV). All K-edge spectra can be used to sensitively detect the dark states. Our proposed O 1s feature has already been observed in a recent TXAS experiment with thymine. Natural transition orbital analysis is used to interpret all dominant features of the three lowest-valence states along the reaction coordinate and reveal some important valence fine-structure information from the core excitation.}
}

