@article{stairMultireferenceQuantumKrylov2020,
  title = {A {{Multireference Quantum Krylov Algorithm}} for {{Strongly Correlated Electrons}}},
  author = {Stair, Nicholas H. and Huang, Renke and Evangelista, Francesco A.},
  year = {2020},
