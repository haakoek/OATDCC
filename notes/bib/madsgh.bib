@article{kowalskiMethodMomentsCoupledcluster2000,
  title = {The Method of Moments of Coupled-Cluster Equations and the Renormalized {{CCSD}}[{{T}}], {{CCSD}}({{T}}), {{CCSD}}({{TQ}}), and {{CCSDT}}({{Q}}) Approaches},
